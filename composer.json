{
    "name": "digitalstate/platform-bpm-camunda-bundle",
    "type": "symfony-bundle",
    "description": "DigitalState Bpm Camunda Bundle",
    "homepage": "https://github.com/DigitalState/Platform-Bpm-Camunda-Bundle.git",
    "keywords": ["Oro", "Bpm"],
    "license": "MIT",
    "authors": [
        {
            "name": "DigitalState, Inc",
            "homepage": "http://www.digitalstate.ca"
        }
    ],
    "require": {
        "php": ">=5.6",
        "digitalstate/platform-bpm-bundle": "dev-master",
        "digitalstate/platform-security-bundle": "dev-master",
        "digitalstate/platform-user-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "Ds\\Bundle\\BpmCamundaBundle": "" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
